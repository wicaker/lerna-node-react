{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "current"
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-object-assign",
    "@babel/proposal-object-rest-spread",
    ["@babel/proposal-class-properties", {
      "spec": true
    }]
  ]
}